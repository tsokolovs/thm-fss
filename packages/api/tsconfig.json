{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "outDir": "./dist",
    "paths": {
      "@routes/*": [
        "routes/*"
      ],
      "@middlewares/*": [
        "middlewares/*"
      ],
      "@db/*": [
        "db/*"
      ]
    }
  }
}
